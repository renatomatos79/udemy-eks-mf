{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "Node16",
      "target": "es5",
      "allowJs": true,
      "moduleResolution": "node16",
      "sourceMap": true,
      "esModuleInterop": true,
      "strict": true,
      "declaration": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*"]
  }